										<input type="text" id="inputUser" placeholder="Username or Email">
									</div>
								</div>
								<div class="control-group">
									<label class="control-label" for="inputPass">Password</label>
									<div class="controls">
										<input type="password" id="inputPass" placeholder="Password">
									</div>
								</div>
								<div class="control-group">
									<div class="controls">
										<label class="checkbox">
											<input type="checkbox"> Remember me
										</label>
										<button type="submit" class="btn btn-success">Login</button>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
